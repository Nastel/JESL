<!-- 
These sets are required to setup for the bayes simulation. Run as an upsert in Query Browser.

upsert set name = "RiskLoosingCustomer", learnquery = "Get Activity Fields appl, properties('iOSVersion'), properties('model'), properties('freeMemory') where name='CancelAccount'", classifier="CustomerSatisfaction"
upsert set name = "SatisfiedCustomer", learnquery = "Get Activity Fields appl, properties('iOSVersion'), properties('model'), properties('freeMemory') where name='PlaceOrder'", classifier="CustomerSatisfaction"
upsert BayesSourceFields active=true, name='appl', jkqlexpr='appl'
upsert BayesSourceFields active=true, name='iOSVerion', jkqlexpr='properties(\'iOSVersion\')'
upsert BayesSourceFields active=true, name='msgItems', jkqlexpr='properties(\'freeMemory\')'
upsert BayesSourceFields active=true, name='model', jkqlexpr='properties(\'model\')'
upsert dictionary properties=('source'='appl,iOSVersion,freeMemory,model'), name='CustomerSatisfaction'	
upsert dictionary properties+=('criteria'='activityname=\'TRACKING_ACTIVITY\'') where name='CustomerSatisfaction' 
-->
 
 
 